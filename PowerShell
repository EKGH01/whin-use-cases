$headers=@{}
$headers.Add("content-type", "application/json")
$response = Invoke-RestMethod -Uri 'https://whin.inutil.info/whin' -Method POST -Headers $headers -ContentType 'application/json' -Body '{    "phone": "YOUR_NUMBER_GOES_HERE",    "token": "YOUR_TOKEN_GOES_HERE",    "text": "HELLO FROM WHIN!!!!!"}'
